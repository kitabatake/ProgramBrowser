<h1>create new program</h1>

<%= form_for( @program ) do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :outline %>
  <%= f.text_field :outline %>

  <%= f.label :git_url %>
  <%= f.text_field :git_url %>

  <div style='margin-top: 20px;'>
    <%= f.submit "Create", class: "btn btn-large btn-primary" %>
  </div>
<% end %>