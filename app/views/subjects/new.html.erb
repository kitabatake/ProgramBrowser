<%= render partial: '/programs/header', locals: {program: @subject.program} %>

<h2> create new Subject </h2>
<%= form_for( [@subject.program, @subject] ) do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <div style='margin-top: 20px;'>
    <%= f.submit "Create", class: "btn btn-large btn-primary" %>
  </div>
<% end %>